<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
     <record id="sequence_session" model="ir.sequence">
        <field name="name">Session Number</field>
        <field name="code">session.number</field>
        <field name="prefix">S</field>
        <field name="padding">4</field>  <!-- Espacios que habra en pre el prefix y el numero-->
        <field name="number_increment">1</field> <!-- cada cuanto sube-->
        <field name="number_next">1</field> <!-- cual es el num siguiente o el inicial-->
     </record>
    </data>
</odoo>
